ros2 run arena_camera_node run --ros-args -p serial:=904240001 -p topic:=newnewnew -p width:=100 -p height:=100 -p pixelformat:=rgb8 -p exposure_auto:=false -p exposure_time:=50 -p trigger_mode:=true
