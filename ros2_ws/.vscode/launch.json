    "version": "0.2.0",
    "configurations": [
        {
            "name": "node : run",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/arena_camera_node/arena_camera_node/run.py",
            "console": "integratedTerminal",
            "args": [
                "--ros-args",
                "-p",
                "serial:=904240001",
                "-p",
                "topic:=newnewnew",
                "-p",
                "width:=100",
                "-p",
                "height:=100",
                "-p",
                "pixelformat:=rgb8",
                "-p",
                "exposure_auto:=false",
                "-p",
                "exposure_time:=\"50\"",
                "-p",
                "trigger_mode:=true",
            ],
            "justMyCode": false,
            "pythonPath": "${workspaceFolder}/ve_dev_debug/bin/python" // for numpy
        },
        {
            "name": "Python: setup",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/arena_camera_node/setup.py",
            "args": [
                ""
            ],
            "console": "integratedTerminal"
        }
    ]
}